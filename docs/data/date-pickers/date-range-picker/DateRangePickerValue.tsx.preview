<GridItem label="Uncontrolled picker" spacing={2}>
  <NextDateRangePicker
    defaultValue={[dayjs('2022-04-07'), dayjs('2022-04-10')]}
  />
</GridItem>
<GridItem label="Controlled picker" spacing={2}>
  <NextDateRangePicker
    value={value}
    onChange={(newValue) => setValue(newValue)}
  />
</GridItem>