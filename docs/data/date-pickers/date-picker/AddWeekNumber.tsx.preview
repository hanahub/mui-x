<StaticDatePicker
  displayWeekNumber
  label="Basic example"
  value={value}
  onChange={(newValue) => {
    setValue(newValue);
  }}
  renderInput={(params) => <TextField {...params} />}
  localeText={{
    calendarWeekNumberHeaderText: '#',
    calendarWeekNumberText: (weekNumber) => `${weekNumber}.`,
  }}
/>